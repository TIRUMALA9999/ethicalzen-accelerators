# EthicalZen Healthcare Guardrail Demo — Environment Configuration
# Copy this file to .env and fill in your values:
#   cp .env.example .env

# Gateway (local Docker or remote VM)
GATEWAY_HOST=localhost
GATEWAY_PORT=8080
SG_PORT=3001

# EthicalZen Backend (get your key at https://ethicalzen.ai/signup)
ETHICALZEN_API_KEY=
ETHICALZEN_BACKEND_URL=https://api.ethicalzen.ai

# Demo tenant
DEMO_TENANT_ID=demo

# OpenAI (optional — enables live proxy tests through the gateway)
OPENAI_API_KEY=
